{
  "name": "postcss-extract",
  "version": "1.0.1",
  "description": "PostCSS plugin to extract css inside @at-rules into separate files",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "postcss-extract",
    "extract"
  ],
  "author": "Maxim Samoilov <samoilowmaxim@gmail.com>",
  "license": "MIT",
  "repository": "Nitive/postcss-extract",
  "bugs": {
    "url": "https://github.com/Nitive/postcss-extract/issues"
  },
  "homepage": "https://github.com/Nitive/postcss-extract",
  "main": "build/index.js",
  "files": [
    "src",
    "build",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "dependencies": {
    "postcss": "^5.0.19",
    "postcss-nesting": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "6.6.5",
    "babel-core": "6.7.4",
    "babel-eslint": "6.0.0",
    "babel-istanbul": "0.7.0",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.6.5",
    "chai": "3.5.0",
    "codecov": "1.0.1",
    "eslint": "2.5.3",
    "eslint-config-airbnb": "6.2.0",
    "mocha": "2.4.5"
  },
  "scripts": {
    "build": "babel src -d build",
    "prepublish": "npm run build",
    "test": "mocha --compilers js:babel-register test/test.js",
    "posttest": "eslint src test",
    "start": "npm test -- -w"
